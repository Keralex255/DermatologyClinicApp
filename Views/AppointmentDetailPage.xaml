<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DermatologyClinicApp.Views.AppointmentDetailPage"
             Title="Detalii Programare">
    <VerticalStackLayout>
        <Label Text="Data Programării:" />
        <Label Text="{Binding AppointmentDate}" />
        <Label Text="Status:" />
        <Label Text="{Binding Status}" />
        <Button Text="Șterge" Clicked="OnDeleteClicked" />
    </VerticalStackLayout>
</ContentPage>